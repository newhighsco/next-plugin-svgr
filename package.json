{
  "name": "@newhighsco/next-plugin-svgr",
  "description": "Next.js plugin for transforming SVGs into React components using SVGR",
  "version": "1.0.4",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/next-plugin-svgr.git"
  },
  "homepage": "https://github.com/newhighsco/next-plugin-svgr#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/next-plugin-svgr/issues"
  },
  "main": "src/index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "npm-run-all --parallel test:*",
    "test:js": "jest --coverage",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.jsx,.json .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "@svgr/webpack": "5.4.0",
    "file-loader": "6.0.0",
    "url-loader": "4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "@commitlint/cli": "9.1.1",
    "@newhighsco/commitlint-config": "1.0.5",
    "@newhighsco/editor-config": "1.1.1",
    "@newhighsco/eslint-config": "2.2.14",
    "@newhighsco/prettier-config": "2.0.3",
    "@newhighsco/release-config": "1.0.26",
    "babel-jest": "26.2.1",
    "eslint": "7.5.0",
    "husky": "4.2.5",
    "jest": "26.2.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "semantic-release": "17.1.1"
  },
  "peerDependencies": {
    "next": "9.5.1"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "packageRules": [
      {
        "extends": [
          "monorepo:nextjs"
        ],
        "semanticCommitType": "fix"
      }
    ]
  }
}
